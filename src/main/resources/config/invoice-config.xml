<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0">
        <loop startRow="0" endRow="0" items="rawProductItems" var="item"
              varType="org.menesty.ikea.processor.invoice.RawInvoiceProductItem">
            <section startRow="0" endRow="0">
                <mapping row="0" col="0">item.originalArtNumber</mapping>
                <mapping row="0" col="1">item.artNumber</mapping>
                <mapping row="0" col="2">item.name</mapping>
                <mapping row="0" col="4">item.count</mapping>
                <mapping row="0" col="5">item.comment</mapping>
                <mapping row="0" col="6">item.priceStr</mapping>
                <mapping row="0" col="7">item.wat</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                    <cellcheck offset="1"/>
                    <cellcheck offset="2"/>
                    <cellcheck offset="3"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>